generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model OhlcBar {
  id     String @id @default(cuid())
  symbol String
  date   String // ISO yyyy-mm-dd
  open   Float
  high   Float
  low    Float
  close  Float
  
  @@unique([symbol, date])
  @@index([symbol, date])
}